s = res.data\n          this.options.unshift({ ilId: '0', baslik: 'Şehir Seçiniz' })\n        }\n      })\n    },\n    citychange (param) {\n      this.selectedistrict = 0\n      StaticService.getdistrict({\n        data: {\n          methodName: 'ilce',\n          ilId: param\n        }\n      }).then(res => {\n        if (res.data) {\n          this.district = res.data\n          this.district.unshift({ ilceId: '0', baslik: 'İlçe Seçiniz' })\n        }\n      })\n    },\n    companychange(item){\n      debugger\n      item.submanager=true\n      item.manager=false\n     // item.selecttype=\"0\"\n      this.selectedcompany.push(item)\n      debugger\n\n    },\n    checkvalidate () {\n      if (\n        Component.validateemail(this.user.usermail) == false) {\n        Component.showalert('Lütfen Form Verilerinizi Kontrol Ediniz', 'error', 'Tamam')\n        this.validation.usermail = 'is-invalid'\n      } else {\n        this.validation.usermail = 'is-valid'\n      }\n      if (\n        this.user.username.trim() == '' ||\n          this.user.username.trim().length < 3 ||\n          Component.allLetter(this.user.username) != true\n      ) {\n        Component.showalert('Lütfen Form Verilerinizi Kontrol Ediniz', 'error', 'Tamam')\n        this.validation.username = 'is-invalid'\n      } else {\n        this.validation.username = 'is-valid'\n      }\n      if (\n        this.user.userlastname.trim() == '' ||\n          this.user.userlastname.trim().length < 3 ||\n          Component.allLetter(this.user.userlastname) != true\n      ) {\n        Component.showalert('Lütfen Form Verilerinizi Kontrol Ediniz', 'error', 'Tamam')\n        this.validation.userlastname = 'is-invalid'\n      } else {\n        this.validation.userlastname = 'is-valid'\n      }\n\n      /* if  (!Component.checktc(this.registermodel.tcno)){\n                this.validation.tcno=\"is-invalid\"\n              }\n              else{\n                this.validation.tcno=\"is-valid\"\n              } *