       </div>\n              </div>\n            </div>\n            <div style=\"float:right\">\n              <button type=\"button\" class=\"btn btn-success\" @click=\"checkvalidate\"><i class=\"fa fa-check\"></i> Kaydet\n              </button>\n              <button type=\"button\" class=\"btn btn-inverse\">Vazgeç</button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport Component from '../../Plugins/Component'\nimport StaticService from '../../ApiService/StaticService'\nimport CompanyService from '../../ApiService/CompanyService'\nimport Multiselect from 'vue-multiselect'\nexport default {\n  created () {\n    this.getcity()\n    this.getcompanylist()\n    this.$store.commit('setmainpage', 'AnaSayfa')\n    this.$store.commit('setsubpage', 'Yeni Kullanıcı Oluştur')\n    this.$store.commit('setbreadcrumbtitle', 'Yeni Kullanıcı Oluştur')\n  },\n  data () {\n    return {\n      ne: true,\n      selected: '0',\n      selectedistrict: '0',\n      district: [{ ilceId: '0', baslik: 'İlçe Seçiniz' }],\n      options: [],\n      value: [],\n      selectedcompany:[],\n      selecttype: '',\n      optionss: [\n      ],\n      validation: {\n        usermail: '',\n        username: '',\n        userlastname: '',\n        tc: '',\n        password: '',\n        confirmpassword: '',\n        city: '',\n        district: '',\n        adress: '',\n        phone: ''\n      },\n      user: {\n        usermail: '',\n        username: '',\n        userlastname: '',\n        tc: '',\n        password: '',\n        confirmpassword: '',\n        city: '',\n        district: '',\n        adress: '',\n        phone: ''\n      }\n    }\n  },\n  components: {\n    Multiselect\n  },\n  methods: {\n    removecompany(param){\n      this.selectedcompany= this.selectedcompany.filter((x)=>{\n        return x.firmaId!=param.firmaId\n      })\n    },\n    getcompanylist(){\n      CompanyService.getcompanylist(\n        {\n          'data': {\n            kullaniciId: this.$store.getters.getuser.kullaniciId,\n            userToken: this.$store.getters.getuser.userToken,\n            methodName: 'firmalarim'\n          }\n        }\n      ).then((res) => {\n        if (res && res.data && res.data.length) {\n              debugger\n          this.optionss = res.data\n        } else { this.optionss = [] }\n      })\n    },\n    getcity () {\n      StaticService.gettallcity({\n        data: {\n          methodName: 'il'\n        }\n      }).then(res => {\n        if (res.data) {\n          this.options = res.data\n          this.options.unshift({ ilId: '0', baslik: 'Şehir Seçiniz' })\n        }\n      })\n    },\n    citychange (param) {\n      this.selectedistrict = 0\n      StaticService.getdistrict({\n        data: {\n          methodName: 'ilce',\n          ilId: param\n        }\n      }).then(res => {\n        if (res.data) {\n          this.district = res.data\n          this.district.unshift({ ilceId: '0', baslik: 'İlçe Seçiniz' })\n        }\n      })\n    },\n    companychange(item){\n      debugger\n      item.submanager=true\n      item.manager=false\n     // item.selecttype=\"0\"\n      this.selectedcompany.push(item)\n      debugger\n\n    },\n    checkvalidate () {\n      if (\n        Component.validateemail(this.user.usermail) == false) {\n        Component.showalert('Lütfen Form Verilerinizi Kontrol Ediniz', 'error', 'Tamam')\n        this.validation.usermail = 'is-invalid'\n      } else {\n        this.validation.usermail = 'is-valid'\n      }\n      if (\n        this.user.username.trim() == '' ||\n          this.user.username.trim().length < 3 ||\n          Component.allLetter(this.user.username) != true\n      ) {\n        Component.showalert('Lütfen Form Verilerinizi Kontrol Ediniz', 'error', 'Tamam')\n        this.validation.username = 'is-invalid'\n      } else {\n        this.validation.username = 'is-valid'\n      }\n      if (\n        this.user.userlastname.trim() == '' ||\n          this.user.userlastname.trim().length < 3 ||\n          Component.allLetter(this.user.userlastname) != true\n      ) {\n        Component.showalert('Lütfen Form Verilerinizi Kontrol Ediniz', 'error', 'Tamam')\n        this.validation.userlastname = 'is-invalid'\n      } else {\n        this.validation.userlastname = 'is-valid'\n      }\n\n      /* if  (!Component.checktc(this.registermodel.tcno)){\n                this.validation.tcno=\"is-invalid\"\n              }\n              else{\n                this.validation.tcno=\"is-valid\"\n              } */\n\n      if (\n        this.user.password.trim() == '' ||\n          this.user.password.trim().length < 8\n\n      ) {\n        Component.showalert('Lütfen Form Verilerinizi Kontrol Ediniz', 'error', 'Tamam')\n        this.validation.confirmpassword = 'is-invalid'\n        this.validation.password = 'is-invalid'\n      } else if (\n        this.user.confirmpassword.trim() == '' ||\n          this.user.confirmpassword.trim().length < 8\n      ) {\n        Component.showalert('Lütfen Form Verilerinizi Kontrol Ediniz', 'error', 'Tamam')\n        this.validation.confirmpassword = 'is-invalid'\n        this.validation.password = 'is-invalid'\n      } else if (this.user.password.trim() !=\n          this.user.confirmpassword.trim()) {\n        Component.showalert('Lütfen Form Verilerinizi Kontrol Ediniz', 'error', 'Tamam')\n        this.validation.password = 'is-invalid'\n        this.validation.confirmpassword = 'is-invalid'\n        this.ne = !this.ne\n      } else {\n        this.validation.password = 'is-valid'\n        this.validation.confirmpassword = 'is-valid'\n      }\n\n      if (this.selected == 0) {\n        Component.showalert('Lütfen Form Verilerinizi Kontrol Ediniz', 'error', 'Tamam')\n        this.validation.city = 'is-invalid'\n      } else {\n        this.validation.city = 'is-valid'\n      }\n      if (this.selectedistrict == 0) {\n        Component.showalert('Lütfen Form Verilerinizi Kontrol Ediniz', 'error', 'Tamam')\n        this.validation.district = 'is-invalid'\n      } else {\n        this.validation.district = 'is-valid'\n      }\n      if (this.user.adress.trim() == '') {\n        Component.showalert('Lütfen Form Verilerinizi Kontrol Ediniz', 'error', 'Tamam')\n        this.validation.adress = 'is-invalid'\n      } else {\n        this.validation.adress = 'is-valid'\n      }\n      if (\n        this.user.phone.trim() == '' ||\n          this.user.phone.trim().length != 11\n      ) {\n        Component.showalert('Lütfen Form Verilerinizi Kontrol Ediniz', 'error', 'Tamam')\n        this.validation.phone = 'is-invalid'\n      } else {\n        this.validation.phone = 'is-valid'\n      }\n\n      /*   else{\n               this.addnewcompany()\n             }\n     */\n    }\n  }\n}\n</script>\n\n<style>\n</style>\n"]}]}