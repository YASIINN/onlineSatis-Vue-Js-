older=\"Teknokent\"\n                              id=\"comment\"></textarea>\n                    <div class=\"invalid-feedback\">\n                      LÜTFEN   GEÇERLİ BİR ADRES GİRİNİZ\n                    </div>\n                  </div>\n                </div>\n                <!--/span-->\n              </div>\n              <!--/row-->\n              <div class=\"row\">\n                <div class=\"col-md-12\">\n                  <div class=\"form-group\">\n                    <label class=\"control-label\">Telefon *</label>\n                    <input type=\"text\"\n                           onkeypress=\"return event.charCode >= 48 && event.charCode <= 57\"\n                           maxlength=\"11\"\n                           minlength=\"11\"\n                           :class=\"validation.phone\"\n                           v-model=\"company.phone\" class=\"form-control form-control-danger\" placeholder=\"05XXXXXXXXX\">\n                    <div class=\"invalid-feedback\">\n                      LÜTFEN   GEÇERLİ BİR TELEFON NUMARASI GİRİNİZ\n                    </div>\n                  </div>\n                </div>\n                <!--/span-->\n                <div class=\"col-md-12\">\n                  <div class=\"form-group\">\n                    <label class=\"control-label\">Dahili</label>\n                    <input type=\"text\"\n                           onkeypress=\"return event.charCode >= 48 && event.charCode <= 57\"\n                           maxlength=\"11\"\n                           minlength=\"11\"\n                           v-model=\"company.phone2\" class=\"form-control form-control-danger\" placeholder=\"05XXXXXXXXX\">\n                  </div>\n                </div>\n                <!--/span-->\n              </div>\n\n            </div>\n\n            <div style=\"float:right\">\n              <button type=\"button\" class=\"btn btn-success\" @click=\"checkvalidate\"><i class=\"fa fa-check\"></i> Kaydet\n              </button>\n              <button type=\"button\" class=\"btn btn-inverse\">Vazgeç</button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport Component from '../../Plugins/Component'\nimport StaticService from '../../ApiService/StaticService'\nimport CompanyService from '../../ApiService/CompanyService'\n\nexport default {\n  created () {\n    this.getcity()\n\n    this.$store.commit('setmainpage', 'AnaSayfa')\n    this.$store.commit('setsubpage', 'Yeni Firma Oluştur')\n    this.$store.commit('setbreadcrumbtitle', 'Yeni Firma Oluştur')\n  },\n  data () {\n    return {\n      selected: '0',\n      selectedistrict: '0',\n      district: [{ ilceId: '0', baslik: 'İlçe Seçiniz' }],\n      options: [],\n      validation: {\n        companyname: '',\n        tax: '',\n        taxno: '',\n        addres: '',\n        phone: '',\n        phone2: '',\n        city: '',\n        district: ''\n\n      },\n      company: {\n        companyname: '',\n        tax: '',\n        taxno: '',\n        addres: '',\n        phone: '',\n        phone2: ''\n      }\n    }\n  },\n  methods: {\n    getcity () {\n      StaticService.gettallcity({\n        data: {\n          methodName: 'il'\n        }\n      }).then(res => {\n        if (res.data) {\n          this.options = res.data\n          this.options.unshift({ ilId: '0', baslik: 'Şehir Seçiniz' })\n        }\n      })\n    },\n    citychange (param) {\n      this.selectedistrict = 0\n      StaticService.getdistrict({\n        data: {\n          methodName: 'ilce',\n          ilId: param\n        }\n      }).then(res => {\n        if (res.data) {\n          this.district = res.data\n          this.district.unshift({ ilceId: '0', baslik: 'İlçe Seçiniz' })\n        }\n      })\n    },\n    clearcompany () {\n      this.company = {\n        companyname: '',\n        tax: '',\n        taxno: '',\n        addres: '',\n        phone: '',\n        phone2: '',\n        fax: ''\n      }\n      this.selected = 0\n      this.selectedistrict = 0\n      this.district = []\n    },\n    addnewcompany () {\n      CompanyService.addnewcompany(\n        {\n          data: {\n            kullaniciId: this.$store.getters.getuser.kullaniciId,\n            firmaAdi: this.company.companyname.toUpperCase(),\n            vergiDairesi: this.company.tax.toUpperCase(),\n            vergiNo: this.company.taxno,\n            adres: this.company.addres.toUpperCase(),\n            il: this.selected,\n            ilce: this.selectedistrict,\n            telefon: this.company.phone,\n            userToken: this.$store.getters.getuser.userToken,\n            telefon2: this.company.phone2,\n            fax: this.company.fax,\n            methodName: 'firmaTanimla'\n          }\n        }\n      ).then((res) => {\n        if (res.status == 200) {\n          Component.showalert(\n            'Firma Başarıyla Oluşturuldu',\n            'success',\n            'Tamam'\n          )\n          this.clearcompany()\n        } else {\n          Component.showalert(\n            'Firma Oluşturulurken Hata Gerçekleşti Lütfen Daha Sonra Tekrar Deneyin..',\n            'error',\n            'Tamam'\n          )\n        }\n      })\n    },\n    checkvalidate () {\n      if (this.company.companyname.trim() == '' || this.company.companyname.trim().length < 3) {\n        this.validation.companyname = 'is-invalid'\n        Component.showalert('Lütfen Form Verilerini Kontrol Ediniz', 'error', 'Tamam')\n      } else {\n        this.validation.companyname = 'is-valid'\n      }\n\n      if (this.company.tax.trim() == ''\n      ) {\n        this.validation.tax = 'is-invalid'\n        Component.showalert('Lütfen Form Verilerini Kontrol Ediniz', 'error', 'Tamam')\n      } else {\n        this.validation.tax = 'is-valid'\n      }\n\n      if (this.company.taxno.trim() == '') {\n        this.validation.taxno = 'is-invalid'\n        Component.showalert('Lütfen Form Verilerini Kontrol Ediniz', 'error', 'Tamam')\n      } else {\n        this.validation.taxno = 'is-valid'\n      }\n\n      if (this.selected == 0) {\n        this.validation.city = 'is-invalid'\n        Component.showalert('Lütfen Form Verilerini Kontrol Ediniz', 'error', 'Tamam')\n      } else {\n        this.validation.city = 'is-valid'\n      }\n\n      if (this.selectedistrict == 0) {\n        this.validation.district = 'is-invalid'\n        Component.showalert('Lütfen Form Verilerini Kontrol Ediniz', 'error', 'Tamam')\n      } else {\n        this.validation.district = 'is-valid'\n      }\n      if (this.company.addres.trim() == '') {\n        this.validation.adress = 'is-invalid'\n        Component.showalert('Lütfen Form Verilerini Kontrol Ediniz', 'error', 'Tamam')\n      } else {\n        this.validation.adress = 'is-valid'\n      }\n\n      if (this.company.phone.trim() == '' || this.company.phone.trim().length < 10) {\n        this.validation.phone = 'is-invalid'\n        Component.showalert('Lütfen Form Verilerini Kontrol Ediniz', 'error', 'Tamam')\n      } else {\n        this.validation.phone = 'is-valid'\n      }\n\n      /*   else{\n               this.addnewcompany()\n             }\n     */\n    }\n  }\n}\n</script>\n\n<style>\n</style>\n"],"sourceRoot":"src/components/Company"}]}